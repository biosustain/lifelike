{
  "name": "ng2-pdf-viewer",
  "version": "6.1.3",
  "description": "Angular 5+ component for rendering PDF",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod=true",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "test": "ng test --watch false",
    "test:w": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p package.json",
    "docs": "ng build --prod --output-path docs --base-href ng2-pdf-viewer && cp ./docs/index.html ./docs/404.html"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://biosustain@github.com/biosustain/pdf-viewer-lib-src.git"
  },
  "keywords": [
    "angular",
    "pdf",
    "angular",
    "angular-pdf",
    "angular2-pdf",
    "ng2",
    "ng2-pdf",
    "angular5"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "bugs": {
    "url": "https://github.com/biosustain/pdf-viewer-lib-src/issues"
  },
  "homepage": "https://github.com/biosustain/pdf-viewer-lib-src#readme",
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@types/pdfjs-dist": "^2.1.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "pdfjs-dist": "^2.2.228",
    "tslib": "^1.10.0",
    "qtip2": "^3.0.3",
    "jquery-ui-dist": "^1.12.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.23",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/language-service": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.34",
    "@types/node": "^13.7.2",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "rxjs": "~6.5.3",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.0",
    "typescript": "^3.4.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "zone.js": "~0.10.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@types/pdfjs-dist",
      "pdfjs-dist"
    ]
  }
}
