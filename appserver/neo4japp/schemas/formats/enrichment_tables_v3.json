{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "data"
  ],
  "definitions": {
    "safeUrl": {
      "type": "string",
      "minLength": 1,
      "maxLength": 4000,
      "pattern": "(?i)^(https?://.+|mailto:.+)$",
      "$comment": "changing this may have defense-in-depth security implications"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "minLength": 0,
      "maxLength": 20000
    },
    "data": {
      "type": "string",
      "minLength": 0,
      "maxLength": 20000
    },
    "result": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "version",
        "domainInfo",
        "genes"
      ],
      "properties": {
        "version": {
          "const": "1"
        },
        "domainInfo": {
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "labels"
            ],
            "properties": {
              "labels": {
                "type": "array",
                "items": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 200
                }
              }
            }
          }
        },
        "genes": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "imported"
            ],
            "properties": {
              "imported": {
                "type": "string",
                "minLength": 1,
                "maxLength": 200
              },
              "matched": {
                "type": "string",
                "minLength": 1,
                "maxLength": 200
              },
              "fullName": {
                "type": "string",
                "minLength": 1,
                "maxLength": 1000
              },
              "link": {
                "$ref": "#/definitions/safeUrl"
              },
              "domains": {
                "type": "object",
                "additionalProperties": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": [
                      "text",
                      "link"
                    ],
                    "properties": {
                      "text": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 512000
                      },
                      "annotatedText": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 512000
                      },
                      "link": {
                        "$ref": "#/definitions/safeUrl"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "cachedResults": {
      "$schema": "http://json-schema.org/draft-07/schema",
      "type": "object",
      "additionalProperties": true,
      "required": [],
      "properties": {}
    }
  }
}
