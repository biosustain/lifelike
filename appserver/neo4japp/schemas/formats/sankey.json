{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "directed",
    "multigraph",
    "graph",
    "nodes",
    "links"
  ],
  "properties": {
    "directed": {
      "type": "boolean"
    },
    "multigraph": {
      "type": "boolean"
    },
    "graph": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "starts",
        "ends",
        "up",
        "down",
        "endo",
        "endoHigh",
        "up2aak1"
      ],
      "properties": {
        "starts": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "ends": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "up": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "down": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "endo": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "endoHigh": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "up2aak1": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        }
      }
    },
    "nodes": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "name": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "id": {
            "type": "number"
          },
          "displayName": {
            "type": "string"
          }
        }
      }
    },
    "links": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "source": {
            "type": "number"
          },
          "target": {
            "type": "number"
          },
          "pageUp": {
            "type": "number"
          }
        }
      }
    }
  }
}
