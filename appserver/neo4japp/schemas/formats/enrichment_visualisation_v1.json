{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "entitiesList"
  ],
  "properties": {
    "name": {
      "type": [
        "string",
        "null"
      ],
      "minLength": 0,
      "maxLength": 2000,
      "$comment": "deprecated (the filename is the new name)"
    },
    "data": {
      "type": "string",
      "minLength": 0,
      "maxLength": 20000
    },
    "entitiesList": {
      "type": "array"
    },
    "domainsList": {
      "type": "array"
    },
    "organism": {
      "type": [
        "string",
        "null"
      ]
    }
  }
}
